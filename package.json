{
  "name": "temp_init",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:docker": "docker-compose up --build crm-app",
    "dev:full": "docker-compose up --build crm-app mcp-server",
    "dev:production": "docker-compose --profile production up --build",
    "dev:local-db": "docker-compose --profile local-db up --build supabase-local",
    "dev:with-cache": "docker-compose --profile with-cache up --build",
    "build": "next build",
    "build:docker": "docker build -t crm-ai-pro .",
    "monitor:build": "npx tsx scripts/monitor-build.ts",
    "start": "next start",
    "start:docker": "docker-compose up -d",
    "stop": "docker-compose down",
    "stop:full": "docker-compose down -v",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "tunnel": "ngrok http 3000",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest",
    "test:e2e": "playwright test",
    "test:voice-agent": "tsx scripts/test-voice-agent-integration.ts",
    "test:mcp-server": "tsx scripts/test-mcp-server.ts",
    "validate-env": "tsx scripts/validate-environment.ts",
    "test:llm-router": "tsx scripts/test-llm-router-api.ts",
    "test:llm-router-comprehensive": "tsx scripts/test-llm-router-comprehensive.ts",
    "validate:e2e-flows": "tsx scripts/validate-e2e-flows.ts",
    "validate:error-scenarios": "tsx scripts/validate-error-scenarios.ts",
    "validate:e2e-full": "npm run validate:e2e-flows && npm run validate:error-scenarios",
    "setup:dev": "npm run setup:env && npm run setup:db && npm run setup:mcp",
    "setup:env": "cp .env.example .env.local",
    "setup:db": "tsx scripts/setup-development-database.ts",
    "setup:mcp": "cd mcp-server && npm install",
    "elevenlabs:configure": "tsx scripts/configure-elevenlabs-agent.ts",
    "deploy:edge-functions": "supabase functions deploy",
    "logs": "docker-compose logs -f",
    "logs:crm": "docker-compose logs -f crm-app",
    "logs:mcp": "docker-compose logs -f mcp-server",
    "health-check": "tsx scripts/health-check.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.45",
    "@ai-sdk/openai": "^2.0.69",
    "@ai-sdk/react": "^2.0.97",
    "@azure/msal-node": "^3.8.3",
    "@elevenlabs/react": "^0.11.3",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@modelcontextprotocol/sdk": "^1.22.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-context-menu": "^2.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.86.0",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-table": "^8.21.3",
    "ai": "^5.0.97",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dexie": "^4.2.1",
    "dotenv": "^17.2.3",
    "google-auth-library": "^10.5.0",
    "googleapis": "^166.0.0",
    "lucide-react": "^0.554.0",
    "next": "14.2.20",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "recharts": "^3.4.1",
    "resend": "^6.5.2",
    "stripe": "^20.0.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^20",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.20",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "5.9.3"
  }
}
