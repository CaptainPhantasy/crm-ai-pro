{
  "name": "temp_init",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:docker": "docker-compose -f docker/docker-compose.yml up --build crm-app",
    "dev:full": "docker-compose -f docker/docker-compose.yml up --build crm-app mcp-server",
    "dev:production": "docker-compose -f docker/docker-compose.yml --profile production up --build",
    "dev:local-db": "docker-compose -f docker/docker-compose.yml --profile local-db up --build supabase-local",
    "dev:with-cache": "docker-compose -f docker/docker-compose.yml --profile with-cache up --build",
    "build": "next build",
    "build:docker": "docker build -t crm-ai-pro .",
    "monitor:build": "npx tsx scripts/monitor-build.ts",
    "start": "next start",
    "start:docker": "docker-compose -f docker/docker-compose.yml up -d",
    "stop": "docker-compose -f docker/docker-compose.yml down",
    "stop:full": "docker-compose -f docker/docker-compose.yml down -v",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "tunnel": "ngrok http 3000",
    "test": "npm run test:validate && npm run test:api && npm run test:ui",
    "test:validate": "npx tsx tests/setup/environment-validator.ts",
    "test:setup": "npx tsx scripts/seed-comprehensive.ts",
    "seed": "npx tsx scripts/seed-comprehensive.ts",
    "test:api": "vitest --config tests/vitest.config.ts run tests/api/",
    "test:ui": "npm run test:validate && playwright test --config tests/playwright.config.ts tests/ui/",
    "test:e2e": "npm run test:validate && playwright test --config tests/playwright.config.ts",
    "test:unit": "vitest --config tests/vitest.config.ts run",
    "setup:dev": "npm run setup:env && npm run setup:db && npm run setup:mcp",
    "setup:env": "cp .env.example .env.local",
    "setup:db": "tsx scripts/setup-development-database.ts",
    "setup:mcp": "cd mcp-server && npm install",
    "elevenlabs:configure": "tsx scripts/configure-elevenlabs-agent.ts",
    "deploy:edge-functions": "supabase functions deploy",
    "logs": "docker-compose -f docker/docker-compose.yml logs -f",
    "logs:crm": "docker-compose -f docker/docker-compose.yml logs -f crm-app",
    "logs:mcp": "docker-compose -f docker/docker-compose.yml logs -f mcp-server",
    "health-check": "tsx scripts/health-check.ts",
    "geocode-jobs": "tsx scripts/geocode-existing-jobs.ts",
    "test:map-performance": "tsx scripts/test-map-performance.ts",
    "setup:email": "tsx scripts/setup-email-templates.ts"
  },
